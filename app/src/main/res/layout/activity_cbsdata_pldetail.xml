<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_gravity="center"
    android:background="#ffffff"
    android:gravity="center"
    android:orientation="vertical" >

    <!-- 데이터 셋팅 레이아웃 -->

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <LinearLayout
            android:id="@+id/ll_detailView"
            android:layout_width="match_parent"
            android:layout_height="0dip"
            android:layout_weight="8"
            android:orientation="vertical" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1sp"
                android:gravity="center"
                android:text="Large Text" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/comm_textitem140_height"
                android:gravity="center_vertical"
                android:weightSum="10" >

                <TextView
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:text="빌딩번호"
                    android:textColor="@color/comm_boldFont_color"
                    android:textSize="@dimen/comm_listitem_bold_fontSize" />

                <TextView
                    android:id="@+id/tv_cbs_bldgNo"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="7"
                    android:text="Large Text"
                    android:textColor="@color/comm_boldFont_color"
                    android:textSize="@dimen/comm_listitem_bold_fontSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/comm_textitem140_height"
                android:gravity="center_vertical"
                android:weightSum="10" >

                <TextView
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:text="건물명"
                    android:textColor="@color/comm_boldFont_color"
                    android:textSize="@dimen/comm_listitem_bold_fontSize" />

                <TextView
                    android:id="@+id/tv_cbs_bldgNm"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="7"
                    android:text="Large Text"
                    android:textColor="@color/comm_boldFont_color"
                    android:textSize="@dimen/comm_listitem_bold_fontSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/comm_textitem140_height"
                android:gravity="center_horizontal|center_vertical"
                android:weightSum="10" >

                <TextView
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:text="호기정보"
                    android:textColor="@color/comm_boldFont_color"
                    android:textSize="@dimen/comm_listitem_bold_fontSize" />

                <TextView
                    android:id="@+id/tv_cbs_carNo"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="7"
                    android:text="Large Text"
                    android:textColor="@color/comm_boldFont_color"
                    android:textSize="@dimen/comm_listitem_bold_fontSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/comm_textitem140_height"
                android:gravity="center_vertical"
                android:weightSum="10" >

                <TextView
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:text="부서정보"
                    android:textColor="@color/comm_boldFont_color"
                    android:textSize="@dimen/comm_listitem_bold_fontSize" />

                <TextView
                    android:id="@+id/tv_cbs_csDeptNm"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="7"
                    android:text="Large Text"
                    android:textColor="@color/comm_boldFont_color"
                    android:textSize="@dimen/comm_listitem_bold_fontSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/comm_textitem140_height"
                android:gravity="center_vertical"
                android:weightSum="10" >

                <TextView
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:text="점검담당자1"
                    android:textColor="@color/comm_boldFont_color"
                    android:textSize="@dimen/comm_listitem_bold_fontSize" />

                <TextView
                    android:id="@+id/tv_cbs_empNm1"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:text="Large Text"
                    android:textColor="@color/comm_boldFont_color"
                    android:textSize="@dimen/comm_listitem_bold_fontSize" />

                <TextView
                    android:id="@+id/tv_cbs_empHp1" android:autoLink="phone"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="4"
                    android:text="Large Text"
                    android:textColor="@color/comm_boldFont_color"
                    android:textSize="@dimen/comm_listitem_bold_fontSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/comm_textitem140_height"
                android:gravity="center_vertical"
                android:weightSum="10" >

                <TextView
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:text="점검담당자2"
                    android:textColor="@color/comm_boldFont_color"
                    android:textSize="@dimen/comm_listitem_bold_fontSize" />

                <TextView
                    android:id="@+id/tv_cbs_empNm2"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:text="Large Text"
                    android:textColor="@color/comm_boldFont_color"
                    android:textSize="@dimen/comm_listitem_bold_fontSize" />

                <TextView
                    android:id="@+id/tv_cbs_empHp2" android:autoLink="phone"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="4"
                    android:text="Large Text"
                    android:textColor="@color/comm_boldFont_color"
                    android:textSize="@dimen/comm_listitem_bold_fontSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/comm_textitem140_height"
                android:gravity="center_vertical"
                android:weightSum="10" >

                <TextView
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:text="접수일시"
                    android:textColor="@color/comm_boldFont_color"
                    android:textSize="@dimen/comm_listitem_bold_fontSize" />

                <TextView
                    android:id="@+id/tv_cbs_recevTm"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="7"
                    android:text="Large Text"
                    android:textColor="@color/comm_boldFont_color"
                    android:textSize="@dimen/comm_listitem_bold_fontSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/comm_textitem140_height"
                android:gravity="center_vertical"
                android:weightSum="10" >

                <TextView
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:text="도착일시"
                    android:textColor="@color/comm_boldFont_color"
                    android:textSize="@dimen/comm_listitem_bold_fontSize" />

                <TextView
                    android:id="@+id/tv_cbs_arriveTm"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="7"
                    android:text="Large Text"
                    android:textColor="@color/comm_boldFont_color"
                    android:textSize="@dimen/comm_listitem_bold_fontSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/comm_textitem140_height"
                android:gravity="center_vertical"
                android:weightSum="10" >

                <TextView
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:text="완료일시"
                    android:textColor="@color/comm_boldFont_color"
                    android:textSize="@dimen/comm_listitem_bold_fontSize" />

                <TextView
                    android:id="@+id/tv_cbs_completeTm"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="7"
                    android:text="Large Text"
                    android:textColor="@color/comm_boldFont_color"
                    android:textSize="@dimen/comm_listitem_bold_fontSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/comm_textitem140_height"
                android:gravity="center_vertical"
                android:weightSum="10" >

                <TextView
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:text="구출일시"
                    android:textColor="@color/comm_boldFont_color"
                    android:textSize="@dimen/comm_listitem_bold_fontSize" />

                <TextView
                    android:id="@+id/tv_cbs_rescueTm"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="7"
                    android:text="Large Text"
                    android:textColor="@color/comm_boldFont_color"
                    android:textSize="@dimen/comm_listitem_bold_fontSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="처리결과" />

                <TextView
                    android:id="@+id/tv_cbs_statusCd"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Large Text" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/tv_cbs_cbsCd1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Large Text" />

                    <TextView
                        android:id="@+id/tv_cbs_cbsCd2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="7"
                        android:text="Medium Text" />

                    <TextView
                        android:id="@+id/tv_cbs_cbsCd3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="7"
                        android:text="Medium Text" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/tv_cbs_faultCd"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="7"
                        android:text="Medium Text" />

                    <TextView
                        android:id="@+id/tv_cbs_procCd"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="7"
                        android:text="Medium Text" />

                    <TextView
                        android:id="@+id/tv_cbs_dutyCd"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="7"
                        android:text="Medium Text" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:weightSum="10" >

                <TextView
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:text="세부내용" />

                <TextView
                    android:id="@+id/tv_cbs_orderDesc"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="7"
                    android:text="Medium Text" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>