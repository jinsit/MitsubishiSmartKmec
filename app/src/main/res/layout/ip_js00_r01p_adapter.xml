<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
	android:layout_height="@dimen/comm_listitem3_height"
	android:background="@drawable/lv_bg250"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp" 
        android:layout_weight="1"
        android:paddingLeft = "@dimen/comm_listitem_margin"
        android:paddingRight="@dimen/comm_listitem_margin">
        <TextView
            style="style/mosttext"
            android:id="@+id/tv01_js_adapter_workNm"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:gravity="center_vertical" />
        <TextView
            style="@style/text"
            android:id="@+id/tv02_js_adapter_fromToTotalTime"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:gravity="center_vertical|right"
            />
    </LinearLayout>

   <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp" 
        android:layout_weight="1"
        android:paddingLeft = "@dimen/comm_listitem_margin"
        android:paddingRight="@dimen/comm_listitem_margin">
        <TextView
            style="@style/text"
            android:id="@+id/tv03_js_adapter_deptNm"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:gravity="center_vertical"/>
        <TextView
            style="@style/text"
            android:id="@+id/tv04_js_adapter_TBD"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:gravity="center_vertical|right"
            />
    </LinearLayout>
    
   <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp" 
        android:layout_weight="1"
        android:paddingLeft = "@dimen/comm_listitem_margin"
        android:paddingRight="@dimen/comm_listitem_margin">
        <TextView
            style="@style/text"
            android:id="@+id/tv05_js_adapter_BldgNm"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:gravity="center_vertical"/>
        <TextView
            style="@style/text"
            android:id="@+id/tv06_js_adapter_carNo"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:gravity="center_vertical|right"
            />
    </LinearLayout>
    
</LinearLayout>
