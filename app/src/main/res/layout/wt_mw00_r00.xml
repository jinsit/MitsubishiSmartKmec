<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@color/comm_bg"
        android:orientation="vertical" >

        <!-- 데이터 셋팅 레이아웃 -->

        <ScrollView
            android:id="@+id/scrollView1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true" >

            <LinearLayout
                android:id="@+id/ll_detailView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dip" android:layout_weight="1"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/comm_subTitle_height"
                        android:background="@color/comm_subTitleBg"
                        android:weightSum="10" >

                        <TextView
                            android:id="@+id/tv_mw_date"
                            android:layout_width="0dip"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/comm_default_attendantsMargin"
                            android:layout_marginRight="@dimen/comm_default_attendantsMargin"
                            android:layout_weight="5"
                            android:text="일자"
                            android:textColor="@color/comm_popup_bg"
                            android:textSize="@dimen/comm_textitem_66Size"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/tv_mw_workNm"
                            android:layout_width="0dip"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/comm_default_attendantsMargin"
                            android:layout_marginRight="@dimen/comm_default_attendantsMargin"
                            android:layout_weight="5"
                            android:gravity="right"
                            android:text="정기점검"
                            android:textColor="@color/comm_popup_bg"
                            android:textSize="@dimen/comm_textitem_66Size"
                            android:textStyle="bold" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/comm_default_attendantsMargin"
                        android:layout_marginRight="@dimen/comm_default_attendantsMargin"
                        android:gravity="center_vertical"
                        android:minHeight="@dimen/comm_textitem110_height"
                        android:weightSum="10" >

                        <TextView
                            android:id="@+id/tv_mw_bldgInfo"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textColor="@color/comm_boldFont_color"
                            android:textSize="@dimen/comm_textitem_56Size" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/comm_default_attendantsMargin"
                        android:layout_marginRight="@dimen/comm_default_attendantsMargin"
                        android:gravity="center_vertical"
                        android:minHeight="@dimen/comm_textitem110_height"
                        android:weightSum="10" >

                        <TextView
                            android:id="@+id/tv_mw_addr"
                            android:layout_width="0dip"
                            android:layout_height="wrap_content"
                            android:layout_weight="10"
                            android:textSize="@dimen/comm_textitem_56Size" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/comm_default_attendantsMargin"
                        android:layout_marginRight="@dimen/comm_default_attendantsMargin"
                        android:gravity="center_vertical"
                        android:minHeight="@dimen/comm_textitem110_height"
                        android:weightSum="10" >

                        <TextView
                            style="@style/colortext"
                            android:layout_width="0dip"
                            android:layout_height="wrap_content"
                            android:layout_weight="3"
                            android:text="부서정보" />

                        <TextView
                            android:id="@+id/tv_mw_csDeptNm"
                            android:layout_width="0dip"
                            android:layout_height="wrap_content"
                            android:layout_weight="7"
                            android:textSize="@dimen/comm_textitem_56Size" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/comm_default_attendantsMargin"
                        android:layout_marginRight="@dimen/comm_default_attendantsMargin"
                        android:gravity="center_vertical"
                        android:minHeight="@dimen/comm_textitem110_height"
                        android:weightSum="10" >

                        <TextView
                            style="@style/colortext"
                            android:layout_width="0dip"
                            android:layout_height="wrap_content"
                            android:layout_weight="3"
                            android:text="점검담당자1" />

                        <TextView
                            android:id="@+id/tv_mw_empNm1"
                            android:layout_width="0dip"
                            android:layout_height="wrap_content"
                            android:layout_weight="3"
                            android:textSize="@dimen/comm_textitem_56Size" />

                        <TextView
                            android:id="@+id/tv_mw_empHp1"
                            android:layout_width="0dip"
                            android:layout_height="wrap_content"
                            android:layout_weight="4"
                            android:autoLink="phone"
                            android:textSize="@dimen/comm_textitem_56Size" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/comm_default_attendantsMargin"
                        android:layout_marginRight="@dimen/comm_default_attendantsMargin"
                        android:gravity="center_vertical"
                        android:minHeight="@dimen/comm_textitem110_height"
                        android:weightSum="10" >

                        <TextView
                            style="@style/colortext"
                            android:layout_width="0dip"
                            android:layout_height="wrap_content"
                            android:layout_weight="3"
                            android:text="점검담당자2" />

                        <TextView
                            android:id="@+id/tv_mw_empNm2"
                            android:layout_width="0dip"
                            android:layout_height="wrap_content"
                            android:layout_weight="3"
                            android:textSize="@dimen/comm_textitem_56Size" />

                        <TextView
                            android:id="@+id/tv_mw_empHp2"
                            android:layout_width="0dip"
                            android:layout_height="wrap_content"
                            android:layout_weight="4"
                            android:autoLink="phone"
                            android:textSize="@dimen/comm_textitem_56Size" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/comm_default_attendantsMargin"
                        android:layout_marginRight="@dimen/comm_default_attendantsMargin"
                        android:gravity="center_vertical"
                        android:minHeight="@dimen/comm_textitem110_height"
                        android:weightSum="10" >

                        <TextView
                            android:id="@+id/tv_mw_st"
                            android:layout_width="0dip"
                            android:layout_height="wrap_content"
                            android:layout_weight="3"
                            android:text="계획"
                            android:textSize="@dimen/comm_textitem_56Size" />

                        <TextView
                            android:id="@+id/tv_mw_cs_fr"
                            android:layout_width="0dip"
                            android:layout_height="wrap_content"
                            android:layout_weight="4"
                            android:textSize="@dimen/comm_textitem_56Size" />
                    </LinearLayout>

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/tv_textlastlinebg" />

                    <TextView
                        android:id="@+id/tv_mw_moveTm"
                        style="@style/btn_mlistmenutext"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/tv_traveltime"
                        android:gravity="center_vertical|right"
                        android:paddingLeft="@dimen/comm_small_attendantsMargin"
                        android:paddingRight="@dimen/comm_small_attendantsMargin"
                        android:text="이동시간" />

                    <TextView
                        android:id="@+id/tv_mw_arriveTm"
                        style="@style/btn_mlistmenutext"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/tv_arrivaltime"
                        android:gravity="center_vertical|right"
                        android:paddingLeft="@dimen/comm_small_attendantsMargin"
                        android:paddingRight="@dimen/comm_small_attendantsMargin"
                        android:text="도착시간" />

                    <TextView
                        android:id="@+id/tv_mw_completeTm"
                        style="@style/btn_mlistmenutext"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/tv_completiontime"
                        android:gravity="center_vertical|right"
                        android:paddingLeft="@dimen/comm_small_attendantsMargin"
                        android:paddingRight="@dimen/comm_small_attendantsMargin"
                        android:text="완료시간" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <Button
                        android:id="@+id/btn_mw_jobComplete"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/btn_completework"
                     android:gravity="center" android:layout_gravity="bottom"
                        android:paddingLeft="@dimen/comm_small_attendantsMargin"
                        android:paddingRight="@dimen/comm_small_attendantsMargin" />
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>

    <FrameLayout
        android:id="@+id/fl_naviBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_marginTop="0sp"
        android:paddingTop="0sp" >

        <!-- 545sp -->

        <com.jinsit.kmec.widget.HomeNavigation
            android:id="@+id/hn_homeNavi"
            android:layout_width="match_parent"
            android:layout_height="match_parent" 
            android:orientation="vertical">
        </com.jinsit.kmec.widget.HomeNavigation>
    </FrameLayout>

</FrameLayout>