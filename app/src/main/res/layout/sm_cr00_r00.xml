<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <TextView
        	    style="@style/btn_listmenutext"
                android:id="@+id/btn01_cr_writeClaim"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/btn_listmenu01"
                android:text="Claim 등록" 
                android:gravity="center"/>
        <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/bgline"/>

            <TextView
        	    style="@style/btn_listmenutext"
                android:id="@+id/btn02_cr_readClaim"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/btn_listmenu02"
                android:text="Claim 조회" 
                android:gravity="center"/>
        <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/bgline"/>
        </LinearLayout>
    </LinearLayout>

    <FrameLayout
        android:id="@+id/fl_naviBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_marginTop="0sp"
        android:paddingTop="0sp" >

        <!-- 545sp -->

        <com.jinsit.kmec.widget.HomeNavigation
            android:id="@+id/hn_homeNavi"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >
        </com.jinsit.kmec.widget.HomeNavigation>
    </FrameLayout>

</FrameLayout>