<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/comm_popup_bg"
    android:orientation="vertical" >

    <include layout="@layout/comm_dialog_title_bar" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center" >

                <TextView
                    android:id="@+id/btn_fr_trouble"
                    style="@style/btn_mlistmenutext"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"/>

                <TextView
                    android:id="@+id/btn_fr_service"
                    style="@style/btn_mlistmenutext"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center" />
            </LinearLayout>

             <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >
            <TextView
                android:id="@+id/btn_fr_code"
                style="@style/btn_mlistmenutext"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/btn_mlistmenu"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/comm_small_attendantsMargin"
                android:paddingRight="@dimen/comm_small_attendantsMargin"
                android:text="@string/btnstr_fr_code" />

            <TextView
                android:id="@+id/btn_fr_cbsCode1"
                style="@style/btn_mlistmenutext"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/btn_mlistmenu"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/comm_small_attendantsMargin"
                android:paddingRight="@dimen/comm_small_attendantsMargin"
                android:text="@string/btnstr_fr_cbsCode1" />

            <TextView
                android:id="@+id/tv_fr_cbsCode2"
                style="@style/btn_mlistmenutext"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/tv_text180bg"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/comm_small_attendantsMargin"
                android:paddingRight="@dimen/comm_small_attendantsMargin"
                android:text="@string/btnstr_fr_cbsCode2" />

            <TextView
                android:id="@+id/tv_fr_cbsCode3"
                style="@style/btn_mlistmenutext"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/tv_text180bg"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/comm_small_attendantsMargin"
                android:paddingRight="@dimen/comm_small_attendantsMargin"
                android:text="@string/btnstr_fr_cbsCode3" />

            <TextView
                android:id="@+id/btn_fr_faultCode"
                style="@style/btn_mlistmenutext"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/btn_mlistmenu"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/comm_small_attendantsMargin"
                android:paddingRight="@dimen/comm_small_attendantsMargin"
                android:text="@string/btnstr_fr_faultCode" />

            <TextView
                android:id="@+id/btn_fr_procCode"
                style="@style/btn_mlistmenutext"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/btn_mlistmenu"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/comm_small_attendantsMargin"
                android:paddingRight="@dimen/comm_small_attendantsMargin"
                android:text="@string/btnstr_fr_procCode" />

            <TextView
                android:id="@+id/btn_fr_dutyCode"
                style="@style/btn_mlistmenutext"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/btn_mlistmenu"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/comm_small_attendantsMargin"
                android:paddingRight="@dimen/comm_small_attendantsMargin"
                android:text="@string/btnstr_fr_dutyCode" />
              </LinearLayout>
    <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/tv_textlastlinebg" />
        </LinearLayout>
    </ScrollView>

    <TextView
        style="@style/btntext"
        android:id="@+id/btn_cm_ok"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_marginTop="@dimen/comm_default_topMargin"
        android:text="확인"
        android:background="@drawable/btn" />

</LinearLayout>