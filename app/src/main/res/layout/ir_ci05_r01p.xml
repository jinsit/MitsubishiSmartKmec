                        android:text="게약부서" />

                    <TextView
                        android:id="@+id/tv_ci_deptNm"
                        style="@style/text"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginRight="@dimen/comm_default_attendantsMargin"
                        android:layout_weight="10"
                        android:gravity="center_vertical" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/tv_text130bg" >

                    <TextView
                        style="@style/colortext"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/comm_default_attendantsMargin"
                        android:layout_weight="6"
                        android:gravity="center_vertical"
                        android:text="계약번호" />

                    <TextView
                        android:id="@+id/tv_ci_csContrNo"
                        style="@style/text"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginRight="@dimen/comm_default_attendantsMargin"
                        android:layout_weight="10"
                        android:gravity="center_vertical" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/tv_text130bg" >

                    <TextView
                        style="@style/colortext"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/comm_default_attendantsMargin"
                        android:layout_weight="6"
                        android:gravity="center_vertical"
                        android:text="계약형태" />

                    <TextView
                        android:id="@+id/tv_ci_csNm"
                        style="@style/text"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginRight="@dimen/comm_default_attendantsMargin"
                        android:layout_weight="10"
                        android:gravity="center_vertical" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
